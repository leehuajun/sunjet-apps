/*border-top:1px solid #0e90d2;*/
/*}*/

.form-grid.z-row:hover > .z-row-inner {
    /*background: rgb(83, 147, 197);*/
    /*color:#f5f5f5;*/
    /*background-clip: padding-box;*/
    position: relative;
    /*border-top: 1px solid rgb(106, 156, 97);*/
    background-color: rgb(255, 255, 255);
    border-bottom: 1px solid rgb(255, 255, 255);
}

.form-grid.z-grid .z-grid-header, .z-grid .z-column {
    /*background-color: rgb(227, 227, 227);*/
    /*border-bottom: 1px solid rgb(163, 209, 238);*/
}

