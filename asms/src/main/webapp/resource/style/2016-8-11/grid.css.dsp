.z-grid {
    border-top: 2px solid #678cb1;
    border-bottom: 1px solid #6fb3e0;
    margin-bottom: 0px;
}

/*.z-row:hover{*/
/*border-top:1px solid #0e90d2;*/
/*border-top:1px solid #0e90d2;*/
/*}*/
.z-column {
    /* 设置下面属性,标识回行*/
    /*white-space:pre-wrap;*/
}

.z-row > .z-row-inner {
    padding-right: 5px;
    white-space: nowrap;
}

.z-row:hover > .z-row-inner {
    /*background: rgb(83, 147, 197);*/
    /*color:#f5f5f5;*/
    /*background-clip: padding-box;*/
    position: relative;
    /*border-top: 1px solid rgb(106, 156, 97);*/
    /*border-top: 1px solid rgb(56, 88, 120);*/
    background-color: rgb(197, 119, 34);
    border-bottom: 1px solid rgb(56, 88, 120);

}

.z-grid .z-grid-header, .z-grid .z-column {
    background-color: rgb(227, 227, 227);
    /*border-bottom: 1px solid rgb(163, 209, 238);*/
}

.z-auxhead .z-auxheader {
    background-color: rgb(227, 227, 227);
}

.z-auxheader-content {
    padding: 0 0;
    background-color: rgb(227, 227, 227);
}

.z-column-content, .z-row-content, .z-group-content, .z-groupfoot-content, .z-footer-content {
    font-family: Arial, Sans-serif;
    font-size: 12px;
    color: #555;
    padding: 2px 4px;
    /*padding:0px 0px;*/
    line-height: 18px;
    overflow: hidden;
}

.z-row-index {
    width: 60px;
}

.z-grid {
    border-left: 1px solid rgb(221, 221, 221);
    border-right: 1px solid rgb(221, 221, 221);

}

.z-grid .z-row > .z-row-inner,
.z-grid .z-row > .z-cell {
    background-image: none;
    border-left: 0px solid rgb(221, 221, 221);
    border-bottom: 1px solid rgb(221, 221, 221);

}

.z-grid .z-row:hover > .z-row-inner,
.z-grid .z-row:hover > .z-cell,
.z-grid .z-row.z-grid-odd:hover > .z-row-inner,
.z-grid .z-row.z-grid-odd:hover > .z-cell {
    /*background-image: none;*/
    background-color: rgb(236, 236, 236);
    border-bottom: 1px solid rgb(57, 132, 201);
    color: rgb(255, 255, 255);
    cursor: pointer;

}

.z-grid .z-column-content {
    padding: 8px;
    line-height: 1.5;
}

.z-grid .z-row.z-grid-odd > .z-row-inner {
    background-color: rgb(249, 249, 249);
}

.z-grid .z-grid-header,
.z-grid .z-column {
    background-color: rgb(227, 227, 227);
    filter: progid:DXImageTransform.Microsoft.gradient(enabled=false);
}

.z-grid .z-column-content {
    font-family: 'Open Sans';
    font-weight: 700;
    font-size: 12px;
    color: #707070;
}

.z-grid .z-column-content [class*="z-icon-"] {
    margin-right: 2px;
}

.domain.z-grid .z-row > .z-row-inner {
    background-color: #F9F9F9;
    border-color: #DDD;
}

.domain.z-grid .z-row.z-grid-odd > .z-row-inner,
.domain.z-grid .z-row.z-grid-odd > .z-row-inner:hover {

    background-color: transparent;
}

.domain.z-grid .z-row + .z-row > .z-row-inner {
    border-top: 1px solid #DDD;
}

.domain.z-grid .red {
    text-decoration: line-through;
}

.domain.z-grid .green {
    font-weight: bold;
}

.list-grid.z-grid {
    margin-bottom: 0px
}

.form-grid.z-grid {
    /*border-top: 2px solid #678cb1;*/
    /*border-bottom: 2px solid #6fb3e0;*/
    border: none;
    margin-bottom: 0px
}

