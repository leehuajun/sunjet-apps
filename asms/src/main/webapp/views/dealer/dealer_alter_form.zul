                                    </row>
                                    <row style="border-bottom:1px solid rgb(240,240,240)">
                                        <label value="索赔主管电话"/>
                                        <label value="@load(vm.dealer.claimDirectorPhone)"/>
                                        <textbox value="@bind(vm.dealerAlterRequest.claimDirectorPhone)"
                                                 hflex="1"
