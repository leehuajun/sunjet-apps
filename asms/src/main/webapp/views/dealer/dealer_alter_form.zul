                                                             model="@load(vm.otherCollaborations)">
                                                        <listitem label="@load(each.name)"
                                                                  value="@load(each.name)"></listitem>
                                                    </listbox>
                                                </bandpopup>
                                            </bandbox>
                                        </div>
                                    </row>


                                    <row style="border-bottom:1px solid rgb(240,240,240)">
                                        <label value="拟维修我公司产品系列"/>
                                        <label value="@load(vm.dealer.productsOfMaintain)"/>
                                        <div>
