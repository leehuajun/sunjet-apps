                                        <!--hflex="1" placeholder="配件储备表"-->
                                        <!--readonly="@bind(vm.readonly)"/>-->

                                    </row>
                                </rows>
                            </grid>
                        </tabpanel>
                        <tabpanel>
                            <grid oddRowSclass="none" hflex="1" sclass="list-grid">
