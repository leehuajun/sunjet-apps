                                                    </listbox>
                                                </bandpopup>
                                            </bandbox>
                                        </div>
                                        <label value="维修资质"/>
                                        <label value="@load(vm.dealer.qualification)"/>
                                        <div>
                                            <listbox mold="select"
                                                     model="@load(vm.qualifications)"
                                                     hflex="1"
                                                     selectedItem="@bind(vm.dealerAlterRequest.qualification)"
                                                     visible="@bind(vm.readonly eq false)">
                                                <template name="model">
                                                    <listitem left="1" label="@load(each.name)"
                                                              value="@load(each.name)"
                                                              selected="@bind(each.name eq vm.dealerAlterRequest.qualification?true:false)">
                                                    </listitem>
                                                </template>
                                            </listbox>
                                            <textbox hflex="1" visible="@bind(vm.readonly eq true)"
                                                     value="@load(vm.dealerAlterRequest.qualification)"
                                                     readonly="@bind(vm.readonly)"
                                                     placeholder="维修资质"></textbox>
                                        </div>
