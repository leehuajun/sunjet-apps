<!-- 变更申请表单 -->
<window title="服务站变更申请" border="normal" width="1000px" height="600px"
        sizable="true" style="padding-top:0px;padding-left:0px;" position="center,center" closable="true"
        viewModel="@id('vm') @init('com.sunjet.vm.dealer.DealerAlterFormVM')"
        validationMessages="@id('vmsgs')"
        onClose="spaceOwner.detach();">
    <borderlayout>
        <north sclass="c-north" height="30px"
               style="padding-top:15px;border-bottom:1px solid rgb(227,227,227);padding-bottom:10px">
            <include src="/views/macros/flow_form_toolbar.zul"/>
        </north>
        <center sclass="c-center" hflex="true" vflex="true" autoscroll="true">
            <vlayout height="100%">
                <groupbox>
                    <caption>基本信息</caption>
                    <grid oddRowSclass="none" hflex="1" sclass="form-grid">
                        <columns>
                            <column width="70px"/>
                            <column hflex="1"/>
                            <column width="80px"/>
                            <column hflex="1"/>
                            <column width="80px"/>
                            <column hflex="1"/>
                            <column width="80px"/>
                            <column hflex="1"/>
                        </columns>
                        <rows>
                            <row>
                                <label value="服务站编号" hflex="1"></label>
                                <bandbox mold="rounded" autodrop="false" hflex="1" readonly="true"
                                         value="@load(vm.dealer.code)">
                                    <bandpopup width="300px" height="300px">
                                        <vlayout hflex="1" vflex="1">
                                            <textbox value="@bind(vm.keyword)" hflex="1"
                                                     placeholder="输入关键字..按Enter键搜索"
                                                     sclass="input-prepend"
                                                     onOK="@command('searchDealers',model=vm.keyword)"/>
                                            <grid style="border:none" vflex="1" model="@load(vm.dealers)"
                                                  emptyMessage="没有数据">
                                                <columns>
                                                    <column hflex="1" label="服务站编号"/>
                                                    <column hflex="2" label="服务站名称"/>
                                                </columns>
                                                <template name="model">
                                                    <row onClick="@command('selectDealer',model=each)">
                                                        <label value="@load(each.code)"/>
                                                        <label value="@load(each.name)"/>
                                                    </row>
                                                </template>
                                            </grid>
                                        </vlayout>
                                    </bandpopup>
                                </bandbox>
                                <label value="服务站名称"></label>
                                <textbox hflex="1" value="@bind(vm.dealer.name)" readonly="true"/>
                                <label value="法人代表" hflex="1"></label>
                                <textbox value="@load(vm.dealer.legalPerson)" hflex="1"
                                         readonly="true"></textbox>
                                <label value="工商营业执照号" hflex="1"></label>
                                <textbox value="@load(vm.dealer.businessLicenseCode)" hflex="1"
                                         readonly="true"></textbox>
                            </row>
                            <row>
                                <label value="组织机构代码号" hflex="1"></label>
                                <textbox value="@load(vm.dealer.orgCode)" hflex="1" readonly="true"></textbox>
                                <label value="纳税人识别" hflex="1"></label>
                                <textbox value="@load(vm.dealer.taxpayerCode)" hflex="1"
                                         readonly="true"></textbox>
                                <label value="开户银行" hflex="1"></label>
                                <textbox value="@load(vm.dealer.bank)" hflex="1" readonly="true"></textbox>
                                <label value="银行账号"></label>
                                <textbox value="@load(vm.dealer.bankAccount)" hflex="1"
                                         readonly="true"></textbox>
                            </row>
                            <row>
                                <label value="地址"></label>
                                <textbox value="@load(vm.dealer.address)" hflex="1" readonly="true"></textbox>
                                <label value="所在省"></label>
                                <textbox value="@load(vm.dealer.province)" hflex="1" readonly="true"></textbox>
                                <label value="所在市"></label>
                                <textbox value="@load(vm.dealer.city)" hflex="1" readonly="true"></textbox>
                                <label value="变更函"></label>
                                <div align="left">
                                    <a target="_blank"
                                       href="@load(vm.getFilePath(vm.dealerAlterRequest.fileAlteration))"
                                       label="查看" visible="@bind(not empty vm.dealerAlterRequest.fileAlteration)"/>
                                    <button tooltiptext="上传文件" iconSclass="@load(iconVariable.icon_search)"
                                            sclass="btn-warning inner-button"
                                            upload="true,maxsize=5120"
                                            onUpload="@command('doUploadFile',event=event,t='file16')"
                                            disabled="@bind(vm.readonly)"/>
                                    <button iconSclass="@load(iconVariable.icon_delete)"
                                            sclass="btn-warning inner-button" label=""
                                            tooltiptext="删除" visible="@load(!vm.readonly)"
                                            onClick="@command('delUploadFile',t='file16')"
                                            style="padding-left:5px;font-weight:700;font-size:14px;"/>
                                </div>
                            </row>
                            <row>
                                <label value="服务站级别"></label>
                                <textbox value="@load(vm.dealer.level)" hflex="1" readonly="true"></textbox>
                                <label value="父级服务站"></label>
                                <textbox value="@load(vm.dealer.parent.name)" hflex="1"
                                         readonly="true"></textbox>
                                <label value="当前等级"/>
                                <textbox value="@load(vm.dealer.star)" hflex="1" readonly="true"></textbox>
                            </row>
                        </rows>
                    </grid>
                </groupbox>
                <tabbox sclass="ntabbox" hflex="true" vflex="true"
                        style="margin-top:10px;margin-bottom:10px;">
                    <tabs>
                        <tab label="基本信息"/>
                        <tab label="人员信息"/>
                        <tab label="场地信息(平方米)"/>
                        <tab label="图片信息(附件)"/>
                    </tabs>
                    <tabpanels style="border-bottom:1px solid green">
                        <tabpanel>
                            <grid oddRowSclass="none" hflex="1" sclass="list-grid">
                                <columns>
                                    <column width="120px" label="变更项目"/>
                                    <column hflex="1" label="变更前"/>
                                    <column hflex="1" label="变更后"/>
                                    <column width="120px" label="变更项目"/>
                                    <column hflex="1" label="变更前"/>
                                    <column hflex="1" label="变更后"/>
                                </columns>
                                <rows>
                                    <row style="border-bottom:1px solid rgb(240,240,240)">
                                        <label value="电话"/>
                                        <label value="@load(vm.dealer.phone)"/>
                                        <textbox value="@bind(vm.dealerAlterRequest.phone)" hflex="1"
                                                 placeholder="电话" readonly="@bind(vm.readonly)"/>
                                        <label value="是否SGMW体系"/>
                                        <label value="@load(vm.dealer.sgmwSystem eq 'true'?'是':'否')"/>
                                        <div>
                                            <listbox hflex="1"
                                                     mold="select"
                                                     visible="@bind(vm.readonly eq false)"
                                                     selectedItem="@bind(vm.dealerAlterRequest.sgmwSystem)"
                                            >
                                                <listitem label="是" value="true"
                                                          selected="@bind(vm.dealerAlterRequest.sgmwSystem eq 'true')"/>
                                                <listitem label="否" value="false"
                                                          selected="@bind(vm.dealerAlterRequest.sgmwSystem eq 'false')"/>
                                            </listbox>
                                            <textbox hflex="1" visible="@bind(vm.readonly eq true)"
                                                     value="@load(vm.dealerAlterRequest.sgmwSystem eq 'true'?'是':'否')"
                                                     readonly="@bind(vm.readonly)"></textbox>
                                        </div>
                                    </row>
                                    <row style="border-bottom:1px solid rgb(240,240,240)">
                                        <label value="传真"/>
                                        <label value="@load(vm.dealer.fax)"/>
                                        <textbox value="@bind(vm.dealerAlterRequest.fax)" hflex="1"
                                                 placeholder="传真"
                                                 readonly="@bind(vm.readonly)"/>
                                        <!--<label value="父级服务站"/>-->
                                        <!--<label value="@load(vm.dealer.parent)"/>-->
                                        <!--<textbox value="@bind(vm.dealerAlterRequest.parent)" hflex="1"-->
                                        <!--placeholder="父级服务站" readonly="@bind(vm.readonly)"/>-->
                                        <label value="其他合作内容"/>
                                        <label value="@load(vm.dealer.otherCollaboration)"/>
                                        <div>
                                            <bandbox value="@bind(vm.dealerAlterRequest.otherCollaboration)"
