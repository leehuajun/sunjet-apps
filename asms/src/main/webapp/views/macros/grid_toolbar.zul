<hlayout>
    <button iconSclass="@load(iconVariable.icon_edit)" sclass="btn-success inner-button" label=""
            tooltiptext="修改" visible="@load(vm.enableUpdate)"
            onClick="@command('openDialog',entity=each,url=vm.formUrl)"
            style="align:right;padding-left:5px;font-weight:700;font-size:14px;"/>
    <button iconSclass="@load(iconVariable.icon_delete)" sclass="btn-warning inner-button" label=""
            tooltiptext="删除" visible="@load(vm.enableDelete)"
            onClick="@command('deleteEntity',entity=each)"
            style="padding-left:5px;font-weight:700;font-size:14px;"/>
</hlayout>